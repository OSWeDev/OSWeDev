    "scripts": {
        "lint": "tslint --project tsconfig_tslint.json --config tslint_promises.json",
        "lint-global": "tslint --project tsconfig.json",
        "check-deps": "depcruise --validate .dependency-cruiser.json src/**/*",
        "watch": "npm-watch",
        "test-coverage-ci": "nyc node ./node_modules/@playwright/test/cli.js test --config=playwright-ci.config.ts",
        "test-ci": "playwright test --config=playwright-ci.config.ts",
        "test": "playwright test --config=playwright.config.ts",
        "test-coverage": "nyc node -r source-map-support/register ./node_modules/@playwright/test/cli.js test --config=playwright.config.ts > test-reports.xml",
        "tscv": "tsc -v",
        "tsc-watch-tests": "tsc -w -p ./test",
        "clean": "rimraf dist",
        "xcopy_vuejsclient": "xcopy \"src\\vuejsclient\" \"dist\\vuejsclient\\\" /D /E /C /F /K /H /R /Y /I /J",
        "build": "npm run-script clean && tsc --project ./",
        "tsc-watch": "tsc-watch --project ./",
        "project-watch": "npm run-script clean && concurrently --prefix \"[{name}]\" --names \"TSC,VUE\" -c \"bgBlue.bold,bgRed.bold\" \"npm run tsc-watch\" \"npm run watch\"",
        "version": "echo $npm_package_version"
    },
