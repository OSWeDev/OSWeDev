        "tsc-watch-tests": "tsc -w -p ./test",
        "clean": "rimraf dist",
        "xcopy_vuejsclient": "xcopy \"src\\vuejsclient\" \"dist\\vuejsclient\\\" /D /E /C /F /K /H /R /Y /I /J",
        "build": "npm run-script clean && tsc --project ./",
        "tsc-watch": "tsc-watch --project ./",
        "project-watch": "npm run-script clean && concurrently --prefix \"[{name}]\" --names \"TSC,VUE\" -c \"bgBlue.bold,bgRed.bold\" \"npm run tsc-watch\" \"npm run watch\"",
