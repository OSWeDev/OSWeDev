        "build-server": "tsc -p ./src/server",
        "build-generator": "tsc -p ./src/generator",
        "version": "echo $npm_package_version"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts",
            "test/**/*.ts",
            "test/testchai.ts",
            "test/test.ts"
        ],
        "reporter": [
            "html",
            "lcov",
            "text-summary"
        ],
        "cache": true,
        "all": true
    },
    "author": "Wedev",
    "license": "Apache 2.0",
    "dependencies": {
        "@types/node": "^7.0.11",
        "@types/express-formidable": "1.0.2",
        "@types/express-session": "^1.15.5",
        "@types/nodemailer": "^4.6.0",
        "moment": "^2.19.1",
        "ts-node": "4.1.0",
        "compression": "^1.7.2",
        "nodemailer": "^4.6.5",
        "node-sass": "^4.8.3",
        "pg": "^4.4.4",
        "pg-promise": "8.1.0",
        "express": "4.16.3",
        "jquery": "^3.2.1",
        "tsc-watch": "^1.0.11",
        "i18next": "latest",
        "i18next-express-middleware": "latest",
        "http-proxy-middleware": "^0.17.4",
        "express-formidable": "1.0.0",
