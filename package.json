{
    "name": "oswedev",
    "version": "0.3.2",
    "description": "",
    "scripts": {
        "test": "nyc mocha",
        "test-jenkins": "mocha -R xunit > test-reports.xml",
        "test-simple": "mocha",
        "clean": "rimraf dist",
        "copyfiles": "copyfiles --up 1 src/**/*.pug src/**/*.scss dist/",
        "build": "npm run-script clean && tsc --project ./",
        "tsc-watch": "tsc-watch --project ./ --onSuccess \"npm run-script copyfiles\"",
        "version": "echo $npm_package_version"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "extension": [
            ".ts"
        ],
        "include": [
            "src/**/*.ts",
            "test/**/*.ts",
            "test/testchai.ts",
            "test/test.ts"
        ],
        "reporter": [
            "html",
            "lcov",
            "text-summary"
        ],
        "cache": true,
        "all": true
    },
    "author": "Wedev",
    "license": "Apache 2.0",
    "dependencies": {
        "@types/node": "7.0.58",
        "@types/express-formidable": "1.0.2",
        "@types/express-session": "1.15.8",
        "@types/nodemailer": "4.6.0",
        "moment": "2.22.0",
        "ts-node": "4.1.0",
        "compression": "1.7.2",
        "nodemailer": "4.6.5",
        "pg": "4.5.7",
        "jquery": "3.3.1",
        "pg-promise": "8.1.0",
        "express": "4.16.3",
        "i18next": "10.6.0",
        "i18next-express-middleware": "1.1.1",
        "http-proxy-middleware": "0.17.4",
        "express-formidable": "1.0.0",
        "express-http-context": "1.0.2",
        "express-locale": "1.0.5",
        "tslib": "1.9.2",
        "express-session": "1.15.6",
        "session-file-store": "1.2.0",
        "jsonwebtoken": "5.7.0",
        "winston": "2.4.1",
        "jade": "1.11.0",
        "winston-daily-rotate-file": "1.7.2",
        "xlsx": "0.12.2",
        "rimraf": "2.6.2"
    },
    "devDependencies": {
        "@types/chai": "4.1.2",
        "@types/mocha": "2.2.48",
        "bootstrap": "4.1.2",
        "chai": "4.1.2",
        "copyfiles": "2.0.0",
        "mocha": "5.0.0",
        "typescript": "3.0.3",
        "tsc-watch": "1.0.27",
        "screenfull": "3.3.2",
        "select2": "4.0.6-rc.1",
        "select2-bootstrap-theme": "0.1.0-beta.10",
        "v-tooltip": "2.0.0-rc.33",
        "vue": "2.5.17",
