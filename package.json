        "vue-i18n": "7.4.0",
        "vue-multiselect": "2.1.0",
        "vue-resource": "1.2.0",
        "vue-router": "3.0.1",
        "vue-select": "2.4.0",
        "vue-snotify": "3.2.0",
        "vue-tables-2": "1.4.64"
