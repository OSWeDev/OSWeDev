    "name": "oswedev",
    "version": "0.22.68",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/OSWeDev/OSWeDev.git"
    },
    "watch": {
        "xcopy_vuejsclient": {
            "patterns": [
                "src/vuejsclient"
            ],
            "extensions": "*",
            "legacyWatch": true,
            "delay": 500
        }
    },
    "scripts": {
        "check-deps": "depcruise --validate .dependency-cruiser.json src",
        "watch": "npm-watch",
        "test": "nyc mocha",
        "test-jenkins": "nyc mocha --require source-map-support/register -R xunit src/test/**/*.ts > test-reports.xml",
        "test-simple": "mocha --require source-map-support/register dist/test/**/*.js",
        "test-simple-watch": "mocha --watch",
        "tscv": "tsc -v",
        "tsc-watch-tests": "tsc -w -p ./test",
        "clean": "rimraf dist",
        "xcopy_vuejsclient": "xcopy \"src\\vuejsclient\" \"dist\\vuejsclient\\\" /D /E /C /F /K /H /R /Y /I /J",
        "build": "npm run-script clean && tsc --project ./",
        "tsc-watch": "tsc-watch --project ./",
        "project-watch": "npm run-script clean && concurrently --prefix \"[{name}]\" --names \"TSC,WEBPACK\" -c \"bgBlue.bold,bgRed.bold\" \"npm run tsc-watch\" \"npm run watch\"",
        "version": "echo $npm_package_version"
    },
    "mocha": {
        "extension": [
            "ts"
        ]
    },
    "nyc": {
        "check-coverage": false,
        "per-file": true,
        "extension": [
            ".ts"
        ],
        "include": [
            "src/generator/**/*.ts",
            "src/server/**/*.ts",
            "src/shared/**/*.ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "cobertura",
            "html"
        ],
        "cache": true,
        "all": true,
        "sourceMap": true,
        "instrument": true
    },
    "author": "Wedev",
    "license": "Apache-2.0",
