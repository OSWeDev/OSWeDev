        "session-file-store": "^1.0.0",
        "jsonwebtoken": "^5.7.0",
        "winston": "^2.4.0",
        "winston-daily-rotate-file": "^1.7.2",
        "xlsx": "0.12.2",
        "vue": "2.5.16",
        "vue-i18n": "7.4.0"
    },
    "devDependencies": {},
    "_dependencies": {
        "pg-promise": "^3.1.6"
    },
    "engines": {
        "node": ">= 8.9.4",
        "npm": ">= 5.8.0"
