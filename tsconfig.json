    "compilerOptions": {
        "target": "es5",
        "sourceMap": true,
        "strict": false,
        "module": "commonjs",
        "importHelpers": true,
        // "outDir": "../../dist/client",
        "lib": [
            "es2015",
            "dom"
        ],
        // "target": "es6",
        // "sourceMap": true,
        // "module": "commonjs",
