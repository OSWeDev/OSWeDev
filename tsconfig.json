        // ],
        "moduleResolution": "node",
        "types": [
            "node"
        ],
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "outDir": "dist",
        "declaration": true
    },
    "include": [
        "src/server/**/*.ts",
        "src/generator/**/*.ts",
        "src/shared/**/*.ts",
        "src/test/**/*.ts"
    ],
    "exclude": [
        "src/client/public/bower",
        "node_modules"
    ]
}