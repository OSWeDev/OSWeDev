{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "quotemark": [
            false
        ],
        "arrow-parens": [
            true
        ],
        "trailing-comma": [
            false,
            {
                "multiline": "always",
                "singleline": "always"
            }
        ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": [
            false,
            "log",
            "error"
        ],
        "no-var-requires": false,
        "prefer-for-of": false,
        "max-line-length": false,
        "no-consecutive-blank-lines": false,
        "no-empty": false,
        "forin": false,
        "no-shadowed-variable": true,
        "object-literal-shorthand": false, // A reactiver ?
        "prefer-const": false, // A reactiver ?
        "variable-name": false, // A reactiver
        "triple-equals": false,
        "radix": false,
        "no-empty-interface": false,
        "space-before-function-paren": false, // Pose problème avec les generators function* () l'espace est ajouté automatiquement
        "no-string-literal": false, // Super important ça pour le typage repasser en true asap
        "no-var-keyword": false, // Repasser true mais ça demande trop de taf pour le moment, pas le temps de faire ce type de modif
        "only-arrow-functions": false, // Repasser true mais ça demande trop de taf pour le moment, pas le temps de faire ce type de modif
        "comment-format": false, // Repasser true mais ça demande trop de taf pour le moment, pas le temps de faire ce type de modif
        "experimentalDecorators": false
    },
    "autoFixOnSave": true,
    "rulesDirectory": []
}