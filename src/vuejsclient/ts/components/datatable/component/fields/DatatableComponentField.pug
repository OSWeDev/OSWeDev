.field(:field_name="field.datatable_field_uid" :class='(field.type == "Simple") ? "field_type_" + field.moduleTableField.field_type : ""' :data-label="t(field.translatable_title)")
    template(v-if="is_load")
        template(v-if='!field.hidden')
            template(v-if="show_label")
                span {{ t(field.translatable_title) }} :
            template(v-if="field.type == 'Simple'")
                template(v-if="field.moduleTableField.field_type == 'boolean'")
                    template(v-if="field_value == 'true' || field_value === true")
                        i.boolean_value.is_true.fa(:class="{[field.moduleTableField.boolean_icon_true]: true, 'text-success': !field.moduleTableField.boolean_invert_colors, 'text-danger': field.moduleTableField.boolean_invert_colors}")
                    template(v-if="field_value == 'false' || field_value === false")
                        i.boolean_value.is_false.fa(:class="{[field.moduleTableField.boolean_icon_false]: true, 'text-success': field.moduleTableField.boolean_invert_colors, 'text-danger': !field.moduleTableField.boolean_invert_colors}")
                template(v-else-if="field.moduleTableField.field_type == 'password'")
                    i.fa.fa-lock
                template(v-else-if="field.moduleTableField.field_type == 'number[]'")
                    ul.multi-input(v-if="field_value")
                        li(v-for="value in field_value") {{ value }}
                template(v-else-if="field.moduleTableField.field_type == 'float[]'")
                    ul.multi-input(v-if="field_value")
                        li(v-for="value in field_value") {{ value }}
                
                template(v-else-if="field.moduleTableField.field_type == 'file'")
                    Filedatatablefieldcomponent(:file_path="field_value", :show_tooltip="show_tooltip")

                template(v-else-if="field.moduleTableField.field_type == 'text[]'")
                    ul.multi-input(v-if="field_value")
                        li(v-for="value in field_value") {{ value }}
                template(v-else-if="field.moduleTableField.field_type == 'html[]'")
                    ul.multi-input(v-if="field_value")
                        li(v-for="value in field_value")
                            template(v-if="explicit_html")
                                span(v-html="value")
                            template(v-else)
                                code {{ value }}
                template(v-else-if="field.moduleTableField.field_type == 'html'")
                    template(v-if="explicit_html")
                        span(v-html="field_value")
                    template(v-else)
                        code {{ field_value }}
                template(v-else-if="field.moduleTableField.field_type == 'enum'")
                    span(:class="transliterate_enum_value_to_class_name") {{ field_value }}
                template(v-else-if="is_custom_field_type && custom_field_types.read_component")
                    .custom_read_field_component(:is="custom_field_types.read_component" :row='vo' :value='field_value')
                template(v-else-if="field.moduleTableField.field_type == 'textarea'")
                    pre {{ field_value }}
                template(v-else)
                    span {{ field_value }}
            template(v-else-if="field.type == 'ManyToOne'")
                template(v-if="field_value")
                    template(v-if="has_access_DAO_ACCESS_TYPE_INSERT_OR_UPDATE && !disabled_many_to_one_link")
                        template(v-if="routeExists(get_crud_link(field.targetModuleTable.vo_type, vo[field.datatable_field_uid + '___id___']))")
                            router-link.btn.btn-sm.btn-info.many_to_one(:to="get_crud_link(field.targetModuleTable.vo_type, vo[field.datatable_field_uid + '___id___'])" :target="!is_dashboard_builder ? '_blank' : ''")
                                img(src='/vuejsclient/public/img/many_to_one.svg')
                                | {{ field_value }}
                    template(v-else)
                        span {{ field_value }}

                    template(v-else)
                        span {{ field_value }}
            template(v-else-if="(field.type == 'ManyToMany') || (field.type == 'OneToMany') || (field.type == 'RefRanges')")
                template(v-if="field_value")
                    template(v-for="e in field_value")
                        template(v-if="!disabled_many_to_one_link && routeExists(get_crud_link(field.targetModuleTable.vo_type, e.id)) && has_access_DAO_ACCESS_TYPE_INSERT_OR_UPDATE")
                            router-link.many-router-links.btn.btn-sm.btn-info(:class='["many_to_many", "one_to_many"][field.type == "ManyToMany" ? 0 : 1]' :to="get_crud_link(field.targetModuleTable.vo_type, e.id)" :target="!is_dashboard_builder ? '_blank' : ''")
                                img(:src='["/vuejsclient/public/img/many_to_many.svg", "/vuejsclient/public/img/one_to_many.svg"][field.type == "ManyToMany" ? 0 : 1]')
                                | {{ e.label }}
                        template(v-else)
                            span {{ e.label }}

            template(v-else-if="field.type == 'COMPONENT'")
                div(:is="field.component" :vo="vo" :columns="columns" :param="vo[field.parameter_datatable_field_uid]" :param_ref_id="vo[field.parameter_datatable_field_uid + '___id___']")

            template(v-else-if="field.type == 'FILE'")
                Filedatatablefieldcomponent(:file_id="vo[field.parameter_datatable_field_uid]")

            template(v-else-if="field.type == 'VAR'")
                Dbvardatatablefieldcomponent(
                    :filter_custom_field_filters="filter_custom_field_filters"
                    :var_id="field.var_id"
                    :filter_type="field.filter_type"
                    :filter_additional_params="field.filter_additional_params"
                    :dashboard_id="field.dashboard_id"
                    :row_value="vo"
                    :columns="columns")

            template(v-else)
                span {{ field_value }}