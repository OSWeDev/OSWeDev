.col-md-12.default
    div(v-if="columnData.trend")
        trend_widget(:positive_min="0.1", :medium_min="0", :column_data="columnData")
        
    div(v-else)
        .row
            span {{ columnData.value }}
        .row(v-if="columnData.moyenne != null")
            span.italic {{ columnData.moyenne }}