.droppable_vos
    .droppable_vos_wrapper
        .header
            //- .hide_show_switch 
            .title {{ label('droppable_vos.title') }}

        .filters
            .set_filter_by_api_type_id
                input(type='search' :placeholder="label('droppable_vos.set_filter_by_api_type_id.placeholder')" @input='filter_by_api_type_id' v-model="filter_value")

        .api_type_ids 
            template(v-for="api_type_id in api_type_ids")
                //- draggable="true" @dragstart="drag(event, api_type_id)" 
                .api_type_id(:api_type_id='api_type_id' v-show='((filter_value == null) || (filter_value == "")) || (api_type_titles[api_type_id].trim().toLowerCase().indexOf(filter_value.trim().toLowerCase()) >= 0)')
                    .header
                        .icons
                            .api_type_id_type
                                i.fa-solid.fa-table.fa-fw(aria-hidden="true")
                            .api_type_id_selected
                                i.fa-solid.fa-check-circle.fa-fw(aria-hidden="true" v-if="get_selected_vos && get_selected_vos[api_type_id]")
                        .title {{ api_type_titles[api_type_id] }}
