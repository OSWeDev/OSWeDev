.container_reponses
    template(v-for="(reponse, index) in field_values")
        .reponse_item(:reponse="index")
            h2 {{ label('crud.container_reponses.numero') }} {{ index + 1 }}
            template(v-for="field in fields")
                Crudcomponentfield(
                    :field="field",
                    :vo="reponse",
                    :show_insert_or_update_target="false",
                    @onchangevo="on_edit",
                    :auto_update_field_value='true',
                )

            .btn.btn-danger(v-if='!readonly', @click="remove(index)") {{ label('crud.field.custom_field.remove_item') }}
    .btn.btn-success(v-if='!readonly', @click="add") {{ label('crud.field.custom_field.add') }}