nav.navbar.navbar-expand-md.flex-md-nowrap.p-0.shadow.sticky-top(:class='nav_classes')

    a.navbar-brand.col-md-3.col-lg-2.me-0.px-3(href="#")
        slot(name="navbar-brand")
    
    button.navbar-toggler.d-md-none(type="button" data-bs-toggle="collapse" :data-bs-target="use_sidebarmenu ? '#sidebarCollapse' : '#navbarCollapse'" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation")
        .navbar-toggler-icon

    .collapse.navbar-collapse.w-100(id="navbarCollapse")

        ul.navbar-nav.me-auto
        
            li.nav-item
                Navbarmenucomponent(
                    :app_name='app_name + "_nav_bar"'
                    v-if='!use_sidebarmenu'
                )

        ul.navbar-nav.m-0

            li.nav-item.p-md-0.p-2
                button.btn.btn-outline-primary.print-page.w-md-auto.w-100(href="javascript:void(0)", v-if="isPrintable && !is_mobile")
                    template(v-if="printComponent")
                        span
                            component(v-bind:is="printComponent")
                    template(v-else)
                        template(v-if="onprint")
                            font-awesome-icon(icon="fa-light fa-print" @click="onprint")
                        template(v-else)
                            font-awesome-icon(icon="fa-light fa-print" @onclick="window.print()")

            li.nav-item.p-md-0.p-2.ms-md-1(v-if="isExportableToXLSX")
                button.btn.btn-outline-primary.export-page.w-md-auto.w-100(href="javascript:void(0)", @click="export_to_xlsx()")
                    font-awesome-icon(icon="fa-light fa-file-export")
                
            li.nav-item.p-md-0.p-2.ms-md-1(v-if='show_lang_selector && !get_hide_lang_selector')
                Langselectorcomponent()
            
            li.nav-item.p-md-0.p-2.ms-md-1(is='Usernotifsmarkercomponent')
            
            li.nav-item.p-md-0.p-2.ms-md-1(is='Navbarusercomponent')

            li.nav-item.p-md-0.p-2.ms-md-1(is='Documenthandlerbuttoncomponent')
                    
    //- .container-fluid
