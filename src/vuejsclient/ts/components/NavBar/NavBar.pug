nav.navbar.navbar-expand-md(:class='nav_classes')
    .container-fluid
    
        a.navbar-brand(href="#")
            slot(name="navbar-brand")
        
        button.navbar-toggler(type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation")
            .navbar-toggler-icon

        .collapse.navbar-collapse(id="navbarCollapse")

            ul.navbar-nav.me-auto
            
                li.nav-item
                    Menucomponent(
                        :app_name='app_name + "_nav_bar"'
                        :is_sidebar='false'
                    )
                    //- :is_navbar='true'

            ul.navbar-nav.m-0

                li.nav-item
                    button.btn.btn-outline-primary.print-page(href="javascript:void(0)", v-if="isPrintable && !is_mobile")
                        template(v-if="printComponent")
                            span
                                component(v-bind:is="printComponent")
                        template(v-else)
                            template(v-if="onprint")
                                font-awesome-icon(icon="fa-duotone fa-print" @click="onprint")
                            template(v-else)
                                font-awesome-icon(icon="fa-duotone fa-print" @onclick="window.print()")

                li.nav-item.ms-1(v-if="isExportableToXLSX")
                    button.btn.btn-outline-primary.export-page(href="javascript:void(0)", @click="export_to_xlsx()")
                        font-awesome-icon(icon="fa-duotone fa-file-export")
                    
                li.nav-item.ms-1(v-if='show_lang_selector')
                    Langselectorcomponent()
                
                li.nav-item.ms-1(is='Usernotifsmarkercomponent')
                
                li.nav-item.ms-1(is='Navbarusercomponent')

                li.nav-item.ms-1
                    Documenthandlerbuttoncomponent